<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.etc.bussiness.entity.Admin" table="admin">
		<id name="id" type="java.lang.Integer">
			<column name="id"/>
			<generator class="native"/>
		</id>
		<property name="aid" type="java.lang.String">
			<column name="aid"/>
		</property>
		<property name="aname" type="java.lang.String">
			<column name="aname"/>
		</property>
		<property name="apassword" type="java.lang.String">
			<column name="apassword"/>
		</property>
		<property name="aphone" type="java.lang.Integer">
			<column name="aphone"/>
		</property>
		<property name="asex" type="java.lang.String">
			<column name="asex"/>
		</property>
			
	</class>

</hibernate-mapping>