<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.etc.bussiness.entity.News" table="news">
		<id name="newsId" type="java.lang.Integer">
			<column name="nid"/>
			<generator class="native"/>
		</id>
		<property name="newsTitle" type="java.lang.String">
			<column name="ntitle"/>
		</property>
		<property name="newsTime" type="java.sql.Timestamp">
			<column name="ntime"/>
		</property>
		<property name="newsDetails" type="java.lang.String">
			<column name="ndetail"/>
		</property>
		<property name="newsImage" type="java.lang.String">
			<column name="nimage"/>
		</property>
			
	</class>

</hibernate-mapping>