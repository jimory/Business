package com.etc.bussiness.entity;

import java.util.Date;


public class Comment {
	private Integer cmid;
	private String cmmessage;
	private Date cmtime;
	private Blog blog;
	private User user;
	public Integer getCmid() {
		return cmid;
	}
	public void setCmid(Integer cmid) {
		this.cmid = cmid;
	}
	public String getCmmessage() {
		return cmmessage;
	}
	public void setCmmessage(String cmmessage) {
		this.cmmessage = cmmessage;
	}
	public Date getCmtime() {
		return cmtime;
	}
	public void setCmtime(Date cmtime) {
		this.cmtime = cmtime;
	}
	public Blog getBlog() {
		return blog;
	}
	public void setBlog(Blog blog) {
		this.blog = blog;
	}
	public User getUser() {
		return user;
	}
	public void setUser(User user) {
		this.user = user;
	}
	public Comment(Integer cmid, String cmmessage, Date cmtime, Blog blog,
			User user) {
		super();
		this.cmid = cmid;
		this.cmmessage = cmmessage;
		this.cmtime = cmtime;
		this.blog = blog;
		this.user = user;
	}
	public Comment(String cmmessage, Date cmtime, Blog blog, User user) {
		super();
		this.cmmessage = cmmessage;
		this.cmtime = cmtime;
		this.blog = blog;
		this.user = user;
	}
	public Comment() {
		super();
	}
	
	

}
