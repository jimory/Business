<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.etc.bussiness.entity.Cases" table="cases">
		<id name="caseId" type="java.lang.Integer">
			<column name="cid"/>
			<generator class="native"/>
		</id>
		<property name="caseTitle" type="java.lang.String">
			<column name="ctitle"/>
		</property>
		<property name="caseDetail" type="java.lang.String">
			<column name="cdetail"/>
		</property>
		<property name="caseImage" type="java.lang.String">
			<column name="cimage"/>
		</property>
			
	</class>

</hibernate-mapping>