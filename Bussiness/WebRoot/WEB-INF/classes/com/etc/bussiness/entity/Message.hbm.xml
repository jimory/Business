<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.etc.bussiness.entity.Message" table="message">
		<id name="mid" type="java.lang.Integer">
			<generator class="native"/>
		</id>
		<property name="mname" type="java.lang.String" />
		<property name="memail" type="java.lang.String" />
		<property name="mphone" type="java.lang.String" />
		<property name="mtext" type="java.lang.String"/>
		<property name="mtime" />
			
	</class>

</hibernate-mapping>